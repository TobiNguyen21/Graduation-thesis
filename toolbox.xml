<xml id="toolbox" style="display: none">
    <category name="%{BKY_CATVARIABLES}" colour="%{BKY_VARIABLES_HUE}" custom="VARIABLE"></category>
    <category name="Output" colour="%{BKY_OUTPUT_HUE}">
        <block type="text_print">
            <value name="TEXT">
                <shadow type="text">
                    <field name="TEXT">abc</field>
                </shadow>
            </value>
        </block>
        <block type="single_character_input"></block>
        <block type="address"></block>
        <block type="pointer"></block>
    </category>
    <category name="Input" colour="%{BKY_INPUT_HUE}">
        <block type="text_scanf">
            <value name="TEXT">
            </value>
        </block>
    </category>

    <sep></sep>

    <category name="%{BKY_CATLOGIC}" colour="%{BKY_LOGIC_HUE}">
        <block type="controls_if"></block>
        <block type="controls_ifelse"></block>
        <block type="logic_compare"></block>
        <block type="logic_operation"></block>
        <block type="logic_negate"></block>
        <block type="logic_boolean"></block>
        <block type="logic_null" disabled="false"></block>
    </category>
    <category name="%{BKY_CATLOOPS}" colour="%{BKY_LOOPS_HUE}">
        <block type="controls_whileUntil"></block>
        <block type="controls_for">
            <value name="FROM">
                <shadow type="math_number">
                    <field name="NUM">0</field>
                </shadow>
            </value>
            <value name="CONDITION">
                <block type="repeat_condition" id="mn)-OQ!u5w/-=JROci1i">
                    <field name="OP">LT</field>
                    <value name="A">
                    </value>
                    <value name="B">
                        <shadow type="math_number">
                            <field name="NUM">10</field>
                        </shadow>
                    </value>
                </block>
            </value>
            <value name="BY">
                <block type="math_post_inc_decrement">
                    <field name="NEWOP">++</field>
                    <value name="VAR">
                    </value>
                </block>
            </value>
        </block>
        <block type="math_prev_inc_decrement_exp"></block>
        <block type="math_post_inc_decrement_exp"></block>
        <block type="math_post_inc_decrement"></block>
        <block type="math_prev_inc_decrement"></block>
        <block type="repeat_condition"></block>
        <block type="controls_flow_statements"></block>
    </category>
    <category name="%{BKY_CATMATH}" colour="%{BKY_MATH_HUE}">
        <block type="math_number" gap="32">
            <field name="NUM">10</field>
        </block>
        <block type="math_arithmetic">
            <value name="A">
                <shadow type="math_number">
                    <field name="NUM">1</field>
                </shadow>
            </value>
            <value name="B">
                <shadow type="math_number">
                    <field name="NUM">1</field>
                </shadow>
            </value>
        </block>
        <block type="operator_bitwise">
            <value name="A">
                <shadow type="math_number">
                    <field name="NUM">1</field>
                </shadow>
            </value>
            <value name="B">
                <shadow type="math_number">
                    <field name="NUM">1</field>
                </shadow>
            </value>
        </block>
        <block type="math_single">
            <value name="NUM">
                <shadow type="math_number">
                    <field name="NUM">9</field>
                </shadow>
            </value>
        </block>
        <block type="math_round">
            <value name="NUM">
                <shadow type="math_number">
                    <field name="NUM">1.4</field>
                </shadow>
            </value>
        </block>
        <block type="math_pow">
            <value name="NUM1">
                <shadow type="math_number">
                    <field name="NUM">2</field>
                </shadow>
            </value>
            <value name="NUM2">
                <shadow type="math_number">
                    <field name="NUM">3</field>
                </shadow>
            </value>
        </block>
        <block type="math_modulo">
            <value name="DIVIDEND">
                <shadow type="math_number">
                    <field name="NUM">99</field>
                </shadow>
            </value>
            <value name="DIVISOR">
                <shadow type="math_number">
                    <field name="NUM">10</field>
                </shadow>
            </value>
        </block>
    </category>
    <category name="%{BKY_CATLISTS}" colour="%{BKY_LISTS_HUE}">
        <block type="variables_array_declare"></block>
        <block type="variables_array_initial">
            <value name="LENGTH">
                <block type="math_number">
                    <field name="NUM">1</field>
                </block>
            </value>
            <value name="ELEMENT">
                <block type="lists_create_empty_v2"></block>
            </value>
        </block>
        <block type="variables_assignment"></block>
        <block type="lists_create_empty_v2"></block>
        <block type="variables_array_get_name"></block>
        <block type="lists_getValueAtIndex"></block>
    </category>
    <category name="%{BKY_CATFUNCTIONS}" colour="%{BKY_PROCEDURES_HUE}" custom="PROCEDURE">
    </category>
</xml>
