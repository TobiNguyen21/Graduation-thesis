                </shadow>
            </value>
        </block> -->
        <block type="text_scanf">
            <value name="TEXT">
            </value>
        </block>
    </category>

    <sep></sep>

    <category name="%{BKY_CATLOGIC}" colour="%{BKY_LOGIC_HUE}">
        <block type="controls_if"></block>
        <block type="controls_ifelse"></block>
        <block type="logic_compare"></block>
        <block type="logic_operation"></block>
        <block type="logic_negate"></block>
        <block type="logic_boolean"></block>
        <block type="logic_null" disabled="false"></block>
    </category>
    <category name="%{BKY_CATLOOPS}" colour="%{BKY_LOOPS_HUE}">
        <block type="controls_whileUntil"></block>
        <block type="controls_for">
            <value name="FROM">
                <shadow type="math_number">
                    <field name="NUM">0</field>
                </shadow>
            </value>
            <value name="CONDITION">
                <block type="repeat_condition" id="mn)-OQ!u5w/-=JROci1i">
					<field name="OP">LT</field>
					<value name="A">
                        <!-- <shadow type="variables_get">
                            <field name="VAR">i</field>
                        </shadow> -->
					</value>
					<value name="B">
						<shadow type="math_number">
							<field name="NUM">10</field>
						</shadow>
					</value>
				</block>
            </value>
            <value name="BY">
                <block type="math_post_inc_decrement">
					<field name="NEWOP">++</field>
					<value name="VAR">
					</value>
				</block>
            </value>
        </block>
        <block type="math_prev_inc_decrement_exp"></block>
        <block type="math_post_inc_decrement_exp"></block>
        <block type="math_post_inc_decrement"></block>
        <block type="math_prev_inc_decrement"></block>
        <block type="repeat_condition"></block>
        <block type="controls_flow_statements"></block>
    </category>
    <category name="%{BKY_CATMATH}" colour="%{BKY_MATH_HUE}">
        <block type="math_number" gap="32">
            <field name="NUM">10</field>
        </block>
        <block type="math_arithmetic">
            <value name="A">
                <shadow type="math_number">
                    <field name="NUM">1</field>
                </shadow>
            </value>
            <value name="B">
                <shadow type="math_number">
                    <field name="NUM">1</field>
                </shadow>
            </value>
        </block>
        <block type="operator_bitwise">
            <value name="A">
                <shadow type="math_number">
                    <field name="NUM">1</field>
                </shadow>
            </value>
            <value name="B">
                <shadow type="math_number">
                    <field name="NUM">1</field>
                </shadow>
            </value>
        </block>
        <block type="math_single">
            <value name="NUM">
                <shadow type="math_number">
                    <field name="NUM">9</field>
                </shadow>
            </value>
        </block>
        <!-- <block type="math_trig">
